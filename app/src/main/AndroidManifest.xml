<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.inventorymaterial">

    <application
        android:name=".iu.inventory.InventoryApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".iu.login.LoginViewImpl"
            android:theme="@style/LoginTheme" />
        <activity android:name=".iu.inventory.InventoryActivity" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".iu.product.ProductActivity" />
        <activity android:name=".iu.dependency.DependecyActivity" />
        <activity android:name=".iu.sector.SectorActivity" />
        <activity android:name=".iu.prefs.GeneralSettingActivity" />
        <activity android:name=".iu.prefs.AccountSettingActivity" />
        <activity android:name=".iu.about.AboutUsActivity" />
        <activity android:name=".iu.dependency.AddDependencyActivity"></activity>
    </application>

</manifest>